<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>

    <add key="DirectLineSecret" value="{Your Direct Line secret from dev.botframework.com}"/>
    <!-- BotId 는 dev.botframework.com 에 봇을 등록할 때 사용한 Bot Handler 값을 사용한다. -->
    <add key="BotId" value="{Your Bot handler which you register at dev.botframework.com}"/>
    <!-- ConversationID 저장을 위한 Azure Cosmos DB 설정-->
    <add key="endpoint" value="{Your Azure cosmos db endpoint url}"/>
    <add key="authKey" value="{Your cosmos db auth key}"/>
    <add key="database" value="{Your database name}"/>
    <add key="collection" value="{Your collection name}"/>
  </appSettings>
  <system.web>
    <customErrors mode="Off"/>
    <compilation debug="true" targetFramework="4.7"/>
    <httpRuntime targetFramework="4.7"/>
    <httpModules>
    </httpModules>
  </system.web>
